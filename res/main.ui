/* /////////////////////////////////////////////////////////////////////////////////////////////////
CheckBox */
QCheckBox::indicator {
	border: 3px solid #5e5e5e;
	width: 15px;
	height: 15px;
	border-radius: 10px;
	background: #f5f7ff;
}
QCheckBox::indicator:hover {
	border: 3px solid #2f93e1
}
QCheckBox::indicator:checked {
	background: 3px solid #2f93e1;
	border: 3px solid #2f93e1;
	background-image: url(:/resources/images/icons/cil-check-alt.png);
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
RadioButton */
QRadioButton::indicator {
	border: 3px solid #5e5e5e;
	width: 15px;
	height: 15px;
	border-radius: 10px;
	background: #f5f7ff;
}
QRadioButton::indicator:hover {
	border: 3px solid rgb(119, 136, 187);
}
QRadioButton::indicator:checked {
	background: 2px solid #2d6d9e;
	border: 3px solid #2f93e1;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
ComboBox */
QComboBox{
	background-color: #ffffff;
	border-radius: 2px;
	border: 1px solid #505050;
	padding: 5px;
	padding-left: 10px;
	color: #424242;
}
QComboBox:hover{
	border: 1px solid #2f93e1;
}
QComboBox::drop-down {
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 25px;
	border-top-right-radius: 3px;
	border-bottom-right-radius: 3px;
	background-image: url(:/resources/images/icons/cil-arrow-bottom.png);
	background-position: center;
	background-repeat: no-reperat;
}
QComboBox QAbstractItemView {
	color: #505050;
	background-color: #ffffff;
	padding: 10px;
	selection-background-color: #2f93e1;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Sliders */
QSlider::groove:horizontal {
	border-radius: 5px;
	height: 10px;
	margin: 0px;
	background-color: #ffffff;
}
QSlider::handle:horizontal {
	background-color: #2f93e1;
	border: none;
	height: 10px;
	width: 10px;
	margin: 0px;
	border-radius: 5px;
}
QSlider::handle:horizontal:hover {
	background-color: rgb(195, 155, 255);
}
QSlider::handle:horizontal:pressed {
	background-color: #2f93e1
}

QSlider::groove:vertical {
	border-radius: 5px;
	width: 10px;
	margin: 0px;
	background-color: #ffffff;
}
QSlider::groove:vertical:hover {
	background-color: #ffffff;
}
QSlider::handle:vertical {
	background-color: rgb(189, 147, 249);
	border: none;
	height: 10px;
	width: 10px;
	margin: 0px;
	border-radius: 5px;
}
QSlider::handle:vertical:hover {
	background-color: rgb(195, 155, 255);
}
QSlider::handle:vertical:pressed {
	background-color: #2f93e1;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
CommandLinkButton */
#pagesContainer QCommandLinkButton {
	color: #2f93e1;
	border-radius: 5px;
	padding: 5px;
	border: 2px solid #2f93e1;
	color: #2f93e1
}
#pagesContainer QCommandLinkButton:hover {
	color: #2f93e1;
	background-color: #ffffff;
}
#pagesContainer QCommandLinkButton:pressed {
	color: rgb(189, 147, 249);
	background-color: #586796;
}

